<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Console" />
		<Option platforms="Windows;Unix;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Win32Debug">
				<Option platforms="Windows;" />
				<Option output="../bin/Consoled" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Debug" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters='toto &quot;t t&quot;' />
				<Compiler>
					<Add option="-g" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_CONSOLE" />
				</Compiler>
				<Linker>
					<Add option="-nostartfiles -nodefaultlibs -nostdlib -ffreestanding" />
					<Add library="win32exd" />
					<Add library="shell32" />
					<Add library="user32" />
					<Add library="kernel32" />
				</Linker>
			</Target>
			<Target title="Win32Release">
				<Option platforms="Windows;" />
				<Option output="../bin/Console" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Release" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DWIN32" />
					<Add option="-DNDEBUG" />
					<Add option="-D_CONSOLE" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-nostartfiles -nodefaultlibs -nostdlib -ffreestanding" />
					<Add library="win32ex" />
					<Add library="shell32" />
					<Add library="user32" />
					<Add library="kernel32" />
				</Linker>
			</Target>
			<Target title="Linux32Debug">
				<Option platforms="Unix;" />
				<Option output="../bin/Consoled" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Debug" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Option parameters='toto &quot;t t&quot;' />
				<Compiler>
					<Add option="-g" />
					<Add option="-m32" />
				</Compiler>
				<Linker>
					<Add option="-m32" />
					<Add library="win32exd" />
				</Linker>
			</Target>
			<Target title="Linux32Release">
				<Option platforms="Unix;" />
				<Option output="../bin/Console" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Release" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m32" />
					<Add option="-pthread" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-m32" />
					<Add library="win32ex" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wall" />
			<Add option="-ansi" />
			<Add option="-mno-stack-arg-probe -fno-stack-check -fno-stack-protector -finput-charset=cp1252 -march=i486 -Wno-long-long" />
			<Add directory="../../win32ex/include" />
		</Compiler>
		<Linker>
			<Add directory="../../win32ex/lib" />
		</Linker>
		<Unit filename="../src/Console/src/Console.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
